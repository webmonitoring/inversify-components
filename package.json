  },
  "typings": "./dts/inversify-components.d.ts",
  "bugs": "https://github.com/webmonitoring/inversify-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/webmonitoring/inversify-components"
  },
  "dependencies": {
