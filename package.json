  "devDependencies": {
    "@types/debug": "^4.1.12",
    "gulp": "^4.0.2",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-tslint": "^8.1.4",
    "gulp-typescript": "^5.0.1",
    "tslint": "^6.1.3",
    "typescript": "~5.3.3"
  },
  "scripts": {
    "publish": "npm publish --registry=https://npm.pkg.github.com",
